        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.showbeatuy.ShowBeatuyActivity"></activity>
        <activity android:name=".FlexBoxLayoutActivity" />
        <activity android:name="com.example.seaechflowlayout.CustomizeActivity" />
        <activity android:name="com.example.search.SearchActivity" />
        <activity android:name="com.example.notificationbar.aty.DaoHangActivity" />
        <activity android:name=".BitmapActivity" />
        <activity android:name=".RuntimePermission.CallActivity" />
        <activity android:name=".SuccessActivity" />
        <activity android:name=".FileActivity" />
        <activity android:name=".SharedPeferencesActivity" />
        <activity android:name=".FilePersistenceActivity" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".Test2Activity" />
        <activity android:name=".TestActivity" />
        <activity android:name="com.example.paotui.activity.LoginActivity"/>
        <activity android:name="com.example.paotui.activity.CheckActivity"/>
        <activity android:name=".ThirdActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIeW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="http" />
            </intent-filter>
        </activity>
        <activity android:name=".IntentActivity" />
